{
  "designSystem": {
    "name": "SabPaisa COB Design System - Enhanced",
    "version": "2.0.0",
    "description": "A comprehensive design system for building consistent, accessible, and beautiful financial applications for SabPaisa's Core Banking platform - Enhanced with multi-format colors, expanded components, and fintech patterns",
    "lastUpdated": "2025-11-25",
    "brandName": "SabPaisa",
    "industry": "FinTech / Core Banking",
    "enhancements": {
      "v2.0.0": [
        "Added HSL color format for all colors",
        "Added RGBA variants with opacity levels (10%, 20%, 50%, 80%, 90%)",
        "Added CSS custom properties format",
        "Added Tailwind CSS class names",
        "Added WCAG contrast ratios for all color combinations",
        "Expanded button component documentation",
        "Added fintech-specific workflow patterns",
        "Expanded component library with modals, dropdowns, tabs, etc.",
        "Added data formatting patterns for amounts, dates, masking"
      ]
    }
  },

  "colorFormats": {
    "description": "All colors are provided in multiple formats for maximum flexibility",
    "formats": {
      "hex": "Hexadecimal format (#RRGGBB)",
      "rgb": "RGB format rgb(r, g, b)",
      "hsl": "HSL format hsl(h, s%, l%)",
      "rgba": "RGBA with opacity variants (10%, 20%, 50%, 80%, 90%)",
      "cssVar": "CSS custom property format (--color-name-shade)",
      "tailwind": "Tailwind CSS class name (color-name-shade)"
    }
  },

